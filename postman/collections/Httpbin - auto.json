		{
			"name": "Set current date",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "df8c1793-8b31-43cb-8f59-f85fda1a2070",
						"exec": [
							"var jsonData = pm.response.json();\r",
							"var Cdate = pm.globals.get(\"CurrentDate\");\r",
							"\r",
							"pm.test(\"Current date is \" + Cdate, function () {\r",
							"\r",
							"    pm.expect(jsonData.json.currentDate).to.eql(Cdate);\r",
							"});\r",
							"\r",
							"var isTrue = false;\r",
							"pm.test(\"Item is exist\", function () {\r",
							"\r",
							"    jsonData.json.permissions.forEach(function(item){\r",
							"    if( item === \"2000\"){\r",
							"    isTrue = true;\r",
							"    pm.expect(item).to.eql(isTrue);\r",
							"    }\r",
							"    });\r",
							"    \r",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "34de2bdb-9745-454c-add2-c21eb2157d4f",
						"exec": [
							"let date = Date.now();\r",
							"\r",
							"pm.globals.set(\"CurrentDate\", date);"
						],
						"type": "text/javascript"
					}
				}
			],
